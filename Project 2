#include <stdio.h>
int main ()
{
int u , v , w , x , y , z ;
printf("A program to calculate an average of 5\n") ;
printf("enter first number\n") ;
scanf("%d" ,&u) ;
printf("enter second number\n") ;
scanf("%d" ,&v) ;
printf("enter thirth number\n") ;
scanf("%d" ,&w) ;
printf("enter fourth number\n") ;
scanf("%d" ,&x) ;
printf("enter fifth number\n") ;
scanf("%d" ,&y) ;
z=(u+v+w+x+y)/5 ;
printf("the average of the number you entered is: %d" ,z) ;
return '0' ;
}
